{
  "functions": [
    {
      "source": ".output/server",
      "runtime": "nodejs18",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        ".env"
      ]
    }
  ],
  "hosting": [
    {
      "site": "website-ormawa",
      "public": ".output/public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "cleanUrls": true,
      "rewrites": [
        {
          "source": "**",
          "function": "server",
          "region": "asia-southeast2"
        }
      ]
    }
  ]
}
